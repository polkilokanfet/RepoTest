<Window x:Class="HVTApp.Services.ChooseProductService.SelectParametersWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HVTApp.Services.ChooseProductService"
        Title="SelectParametersWindow" SizeToContent="WidthAndHeight">
    <StackPanel Orientation="Vertical">
        <ItemsControl Name="ItemsControl">
            <!--<ItemsControl ItemsSource="{Binding UnionsOfParameters}">-->
                <ListBox ItemsSource="{Binding ParametersToSelect}" SelectedItem="{Binding SelectedParameter}"/>
        </ItemsControl>
        <Button>Ok</Button>
    </StackPanel>
</Window>
